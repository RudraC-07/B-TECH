generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "mysql"
}

model product {
  id          Int     @id @default(autoincrement())
  title       String  @db.VarChar(255)
  price       Decimal @db.Decimal(10, 2)
  category    String  @db.VarChar(100)
  description String? @db.Text
}
